
  <%= @user[:name] %>


  <p id="notice"><%= notice %></p>

  <h1>Bookmarks</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Url</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @bookmarks.each do |bookmark| %>
        <tr>
          <td><%= bookmark.name %></td>
          <td><%= bookmark.url %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
